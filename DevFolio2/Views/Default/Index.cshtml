

@{ Layout = null; }

<!DOCTYPE html>
<html lang="en">

@Html.Action("PartialHead", "Default")

<body id="page-top">

    <!--/ Nav Star /-->
    @Html.Action("PartialNavbar", "Default")
    <!--/ Nav End /-->
    <!--/ Intro Skew Star /-->
    @Html.Action("PartialFeature", "Default")
    <!--/ Intro Skew End /-->

    <section id="about" class="about-mf sect-pt4 route">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="box-shadow-full">
                        <div class="row">
                            <div class="col-md-6">
                                @Html.Action("PartialProfile", "Default")

                                @Html.Action("PartialSkill", "Default")
                            </div>
                            <div class="col-md-6">
                                <div class="about-me pt-4 pt-md-0">
                                    <div class="title-box-2">
                                        <h5 class="title-left">
                                            Hakkımda
                                        </h5>
                                    </div>
                                    @Html.Action("PartialAbout", "Default")
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--/ Section Services Star /-->
    <section id="service" class="services-mf route">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="title-box text-center">
                        <h3 class="title-a">
                            Hizmetlerim
                        </h3>
                        <p class="subtitle-a">
                            İlgilendiğim ve gelişmeye çalıştığım alanlar:
                        </p>
                        <div class="line-mf"></div>
                    </div>
                </div>
            </div>
            <div class="row">
                @Html.Action("PartialService", "Default")
            </div>
        </div>
    </section>
    <!--/ Section Services End /-->
    @Html.Action("PartialStatistic", "Default")

    <!--/ Section Portfolio Star /-->
    <section id="work" class="portfolio-mf sect-pt4 route">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="title-box text-center">
                        <h3 class="title-a">
                            Çalışmalarım
                        </h3>
                        <p class="subtitle-a">
                            Projelerim
                        </p>
                        <div class="line-mf"></div>
                    </div>
                </div>
            </div>
            @Html.Action("PartialPortfolio", "Default")
        </div>
    </section>
    <!--/ Section Portfolio End /-->
    <!--/ Section Testimonials Star /-->
    <div class="testimonials paralax-mf bg-image" style="background-image: url(img/overlay-bg.jpg)">
        <div class="overlay-mf"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    @Html.Action("PartialTestimonial", "Default")
                </div>
            </div>
        </div>
    </div>

